{
  "name": "boggler",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "codemirror": "^5.43.0",
    "concurrently": "^4.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.5.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.1",
    "js-file-download": "^0.4.4",
    "markdown-pdf": "^9.0.0",
    "node-deepspeech": "^0.1.5",
    "nodemon": "^1.18.9",
    "open-color": "^1.6.3",
    "progress": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.7.0",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.7.0",
    "react-markdown": "^4.0.6",
    "react-player": "^1.8.0",
    "react-scripts": "2.1.3",
    "react-test-renderer": "^16.8.1",
    "styled-components": "^4.1.3",
    "tar": "^4.4.8"
  },
  "scripts": {
    "start": "concurrently \"yarn nodemon ./src/server/index.js \" \"yarn dev\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "yarn test -w=4 --silent --noStackTrace --forceExit",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "pre": "node preinstall.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "prettier"
    ],
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/prop-types": 0,
      "react/destructuring-assignment": 0,
      "react/require-default-props": 0,
      "import/prefer-default-export": 0,
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/label-has-associated-control": 0,
      "no-console": 0,
      "undefined": 0,
      "import/named": 0,
      "radix": 0
    },
    "env": {
      "browser": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "dotenv": "^6.2.0",
    "wget-improved": "^3.0.2"
  }
}
